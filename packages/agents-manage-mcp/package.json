{
  "name": "@inkeep/agents-manage-mcp",
  "version": "0.40.0",
  "author": "Speakeasy",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "@inkeep/agents-manage-mcp": "bin/mcp-server.js",
    "mcp": "bin/mcp-server.js"
  },
  "scripts": {
    "build": "bun i && bun src/mcp-server/build.mts && tsdown",
    "mcpb:build": "bun i && bun src/mcp-server/build.mts --pack && tsdown",
    "check": "npm run lint",
    "lint": "eslint --cache --max-warnings=0 src",
    "prepublishOnly": "npm run build",
    "generate": "node scripts/generate.mjs"
  },
  "dependencies": {
    "@stricli/core": "^1.1.2",
    "bun": "^1.2.12",
    "express": "^5.1.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/mcpb": "^1.2.0",
    "@eslint/js": "^9.26.0",
    "@types/bun": "^1.2.13",
    "@types/express": "^5.0.1",
    "@types/node": "^18.19.3",
    "@modelcontextprotocol/sdk": "1.24.3",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "tsdown": "^0.18.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1",
    "dotenv": "^16.4.7",
    "find-up": "^7.0.0"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "1.24.3"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkeep/agents.git",
    "directory": "packages/agents-manage-mcp"
  }
}
