{
  "name": "@inkeep/ai-sdk-provider",
  "version": "0.40.0",
  "description": "AI SDK provider for Inkeep Agent Framework",
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "knip": "knip --directory ../.. --workspace packages/ai-sdk-provider --dependencies",
    "build": "tsdown",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "biome lint --error-on-warnings",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "inkeep",
    "agents",
    "provider",
    "llm"
  ],
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "zod": "^4.1.12",
    "@ai-sdk/provider": "3.0.0-beta.22",
    "@ai-sdk/provider-utils": "4.0.0-beta.40",
    "@inkeep/agents-core": "workspace:*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkeep/agents.git",
    "directory": "packages/ai-sdk-provider"
  }
}
